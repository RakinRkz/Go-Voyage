<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1080.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="newproject.HomepageController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1080.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logo.PNG" />
         </image>
      </ImageView>
      <Button fx:id="loginUserBtn" layoutX="461.0" layoutY="349.0" mnemonicParsing="false" onAction="#loginUserBtnOnAction" style="-fx-background-color: #24A0ED; -fx-background-radius: 10;" text="Login as User" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="loginAdminBtn" layoutX="452.0" layoutY="256.0" mnemonicParsing="false" onAction="#loginAdminBtnOnAction" style="-fx-background-color: #24A0ED; -fx-background-radius: 10;" text="Login as Admin" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
